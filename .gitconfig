[user]
        name = Anton Hemlin
        email = anton.hemlin@gmail.com
[alias]
        alias = !git config --global --list | grep ^alias
        br = branch -vvv
        ci = commit
        amend = commit --amend --no-edit
        co = checkout
        head = rev-list -n1 --abbrev-commit HEAD
        hist = log --graph --format=format:\"%C(red)%h%C(reset) %C(yellow)%ad%C(reset) | %s %C(green)\\[%an\\]%C(reset)%C(bold blue)%d%C(reset)\" --abbrev-commit --date=short
        root = rev-parse --show-toplevel
        st = status
        pu = push origin HEAD:refs/for/master
        pu2 = push origin HEAD:refs/for/dev/v2
[core]
        editor = vim
        autocrlf = true
[push]
	default = simple
